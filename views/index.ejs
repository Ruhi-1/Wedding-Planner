<header>
      <div class="navbar navbar-light" style="background-color: rgb(129, 126, 120)">
        <a href="/" class="brand-logo left">Home</a>
        <a href="/guests" class="brand-logo center">guests-list</a>
        <a href="/foods" class="brand-logo center">Foods-list</a>
        <ul class="right">
          <li>
          <% if (user) { %>
            <a href="/logout"><i class="material-icons left">trending_flat</i>Log Out</a>
          <% } else { %>
            <a href="/auth/google"><i class="material-icons left">vpn_key</i>Login with Google</a>
          <% } %>
          </li>
        </ul>
      </div>
</header>
<br>
<body>
  <br>
  <form action="/" method="POST">
    <label class="names">Your Name:
      <input type="text" name="name">
    </label>
    <br>
    <br>
    <label class="names">Wedding Date:
    <input type="date" value= "yyy-MM-dd" name="Wedding-Date">
    </label>
  </form>
</body>